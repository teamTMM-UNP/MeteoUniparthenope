<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Enabled</key>
		<true/>
		<key>RunAtLoad</key>
		<true/>
		<key>KeepAlive</key>
		<true/>
		<key>Label</key>
		<string>nativescript.smtp.server</string>
		<key>EnvironmentVariables</key>
		<dict>
			<key>PATH</key>
			<string>/usr/local/opt/node@8/bin/node:/usr/local/opt/node@8/bin:/bin:/usr/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:$PATH</string>
		</dict>
		<key>ProgramArguments</key>
		<array>
			<string>/usr/local/lib/node_modules/smtp-server/bin/www</string>
			<string>--port 8800</string>
		</array>
		<key>WorkingDirectory</key>
		<string>/</string>
		<key>StandardOutPath</key>
		<string>/Users/nsbuilduser/smtp-server-output.txt</string>
		<key>StandardErrorPath</key>
		<string>/Users/nsbuilduser/smtp-server-errors.txt</string>
		<key>HardResourceLimits</key>
		<dict>
			<key>NumberOfFiles</key>
			<integer>4096</integer>
		</dict>
		<key>SoftResourceLimits</key>
		<dict>
			<key>NumberOfFiles</key>
			<integer>4096</integer>
		</dict>
	</dict>
</plist>
